{
  "version": "3.1.0",
  "name": "@committed/components",
  "description": "Committed Component Library",
  "author": "Committed",
  "license": "MIT",
  "private": false,
  "repository": "git://github.com/commitd/components.git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/components.esm.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --docs",
    "format": "prettier --write '**/{src,test,stories}/**/{*.js,*.ts,*.tsx,*.json,*.md,*.mdx}' example/index.tsx",
    "deploy-storybook": "storybook-to-ghpages",
    "danger:local": "danger local --dangerfile dangerfile.lite.ts",
    "danger:pr": "cross-env DANGER_GITHUB_API_TOKEN=${GITHUB_TOKEN} danger pr https://github.com/commitd/components/pull/${PR}"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(ts|tsx|json|md)'; tsdx lint",
      "pre-push": "yarn build; yarn danger:local"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@compositor/kit": "^1.0.47",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.5",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.33",
    "@types/react-dom": "^16.9.6",
    "babel-loader": "^8.1.0",
    "danger": "^10.1.0",
    "danger-plugin-spellcheck": "^1.5.0",
    "husky": "^4.2.3",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "styled-components": "^5.1.0",
    "ts-loader": "^6.2.2",
    "tsdx": "^0.13.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "clsx": "^1.1.0",
    "deepmerge": "^4.2.2"
  }
}
