{
  "name": "@committed/components",
  "version": "1.0.0",
  "description": "Committed Component Library",
  "author": "committed",
  "license": "UNLICENSED",
  "private": true,
  "repository": "@committed/components",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "prettier": {
    "semi": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern './**/*.*(ts|tsx|json)'",
      "pre-push": "yarn build && yarn test"
    }
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "start:docs": "docz dev",
    "build:docs": "docz build",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "format": "prettier --write '{src/**/*.ts,*.json}'"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/styles": "^4.3.0",
    "@styled-system/theme-get": "^5.0.12",
    "@types/styled-components": "^4.1.16",
    "@types/styled-system": "^4.2.1",
    "styled-components": "^4.3.1",
    "styled-system": "^5.0.10"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@compositor/kit": "^1.0.47",
    "@compositor/kit-snapshot": "^1.0.24",
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-plugin-styled-components": "^1.10.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "gh-pages": "^1.2.0",
    "husky": "^2.5.0",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts-ts": "^2.16.0",
    "remark-emoji": "^2.0.2",
    "remark-external-links": "^5.0.0",
    "remark-images": "^0.18.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-url": "^1.4.0",
    "typescript": "^3.5.3"
  },
  "files": [
    "dist"
  ],
  "resolutions": {
    "styled-components": "4.3.2"
  }
}
