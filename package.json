{
  "version": "0.0.0-development",
  "name": "@committed/components",
  "description": "Committed Component Library",
  "author": "Committed",
  "license": "MIT",
  "private": false,
  "repository": "git://github.com/commitd/components.git",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && node build.js && tsc --project tsconfig.build.json",
    "lint": "tsc --noEmit && eslint './src/**/*.{ts,tsx}'",
    "test": "jest",
    "coverage": "npx live-server coverage/lcov-report",
    "commit": "cz",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -p 6006 -h 0.0.0.0",
    "build-storybook": "build-storybook",
    "format": "prettier --write 'src/**/{*.js,*.ts,*.tsx,*.json,*.md,*.mdx}' example/index.tsx *.js .*.js",
    "format:check": "prettier --check '**/{src,test,stories}/**/{*.js,*.ts,*.tsx,*.json,*.md,*.mdx}' example/index.tsx",
    "deploy-storybook": "storybook-to-ghpages",
    "danger:local": "danger local -b main --dangerfile dangerfile.lite.ts",
    "danger:pr": "danger pr https://github.com/commitd/components/pull/${PR}",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "size-limit": [
    {
      "path": "dist/index.cjs.js",
      "limit": "85 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "85 KB"
    }
  ],
  "lint-staged": {
    "*.{ts,tsx,md,json,mdx}": "prettier --write",
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@faker-js/faker": "^6.1.2",
    "@fontsource/dosis": "^4.5.7",
    "@fontsource/inter": "^4.5.7",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/storybook-deployer": "^2.8.11",
    "@storybook/testing-react": "^1.2.4",
    "@storybook/theming": "^6.4.22",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/parse-color": "^1.0.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.20.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "danger": "^11.0.2",
    "danger-plugin-spellcheck": "^2.1.0",
    "esbuild": "^0.14.36",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-jest-dom": "^3.7.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.10",
    "find-cache-dir": "^3.3.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-sonar-reporter": "^2.0.0",
    "jest-stitches": "^1.0.4",
    "lint-staged": "^12.3.7",
    "parse-color": "^1.0.0",
    "plop": "^3.0.5",
    "pretty-quick": "^3.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-virtual": "^2.10.4",
    "resize-observer-polyfill": "^1.5.1",
    "semantic-release": "^19.0.2",
    "size-limit": "^7.0.8",
    "story-description-loader": "^1.0.0",
    "storybook-dark-mode": "^1.0.9",
    "ts-jest": "^27.1.4",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@committed/hooks": "^0.5.0",
    "@mdi/js": "^6.6.96",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-accordion": "0.1.6",
    "@radix-ui/react-alert-dialog": "0.1.7",
    "@radix-ui/react-avatar": "0.1.4",
    "@radix-ui/react-checkbox": "0.1.5",
    "@radix-ui/react-context-menu": "0.1.6",
    "@radix-ui/react-dialog": "0.1.7",
    "@radix-ui/react-dropdown-menu": "0.1.6",
    "@radix-ui/react-label": "0.1.5",
    "@radix-ui/react-navigation-menu": "0.1.2",
    "@radix-ui/react-polymorphic": "0.0.14",
    "@radix-ui/react-popover": "0.1.6",
    "@radix-ui/react-progress": "0.1.4",
    "@radix-ui/react-radio-group": "0.1.5",
    "@radix-ui/react-select": "0.1.1",
    "@radix-ui/react-separator": "0.1.4",
    "@radix-ui/react-slider": "0.1.4",
    "@radix-ui/react-slot": "^0.1.2",
    "@radix-ui/react-switch": "0.1.5",
    "@radix-ui/react-tabs": "0.1.5",
    "@radix-ui/react-toast": "0.1.1",
    "@radix-ui/react-tooltip": "0.1.7",
    "@radix-ui/react-visually-hidden": "^0.1.4",
    "@stitches/react": "^1.2.7"
  }
}
