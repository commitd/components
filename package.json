{
  "name": "@committed/components",
  "version": "2.0.0",
  "description": "Committed Component Library",
  "author": "Committed",
  "license": "MIT",
  "private": false,
  "repository": "git://github.com/commitd/components.git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern './**/*.*(ts|tsx|json)'",
      "pre-push": "yarn build; yarn danger:local"
    }
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "develop": "yarn storybook",
    "format": "prettier --write '{**/*.js,*.ts,*.tsx,*.json,*.md,*.mdx}'",
    "format-check": "prettier --check '{**/*.js,*.ts,*.tsx,*.json,*.md,*.mdx}'",
    "deploy-storybook": "storybook-to-ghpages",
    "danger:local": "danger local --dangerfile dangerfile.lite.ts",
    "danger:pr": "cross-env DANGER_GITHUB_API_TOKEN=${GITHUB_TOKEN} danger pr https://github.com/commitd/components/pull/${PR}"
  },
  "dependencies": {
    "clsx": "^1.0.4"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.2",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@compositor/kit": "^1.0.47",
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.2.1",
    "@storybook/addon-a11y": "^5.2.6",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@storybook/source-loader": "^5.2.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "@svgr/rollup": "^2.4.1",
    "@svgr/webpack": "^4.3.3",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/storybook__react": "^4.0.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "danger": "^9.2.2",
    "danger-plugin-spellcheck": "^1.5.0",
    "file-loader": "^4.2.0",
    "gh-pages": "^1.2.0",
    "husky": "^2.5.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.9.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.9.0",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-url": "^1.4.0",
    "shx": "^0.3.2",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "url-loader": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "files": [
    "dist"
  ],
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  }
}
